{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "AllowedHosts": "*",
  "UseMemoryDB": false,
  "DefaultConnection": "server=127.0.0.1;uid=root;pwd=;database=short_url",
  "ShortUrlCodeLength": 4,
  "NotFoundRedirectURL": "http://localhost:4200/not-found",
  "NotFoundRedirectIncludeCode": true,
  "ForceHttpsLink": false,

  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      },
      "HttpsDefaultCert": {
        "Url": "https://0.0.0.0:5001"
      }
    }
  }
}
